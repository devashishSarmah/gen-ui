{
  "name": "@gen-ui/design-system",
  "version": "0.1.0",
  "description": "Gen UI Design System - Dark glass theme with Angular Aria headless components",
  "license": "MIT",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "style": "src/lib/index.css",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./styles": "./src/lib/index.css",
    "./tokens": "./src/lib/tokens.css",
    "./components": "./src/lib/components.css",
    "./utilities": "./src/lib/utilities.css"
  },
  "keywords": [
    "design-system",
    "angular",
    "angular-aria",
    "glassmorphism",
    "dark-theme",
    "headless-ui"
  ],
  "peerDependencies": {
    "@angular/core": ">=21.0.0",
    "@angular/common": ">=21.0.0",
    "@angular/aria": ">=21.0.0",
    "@angular/cdk": ">=21.0.0"
  },
  "sideEffects": [
    "*.css"
  ]
}
